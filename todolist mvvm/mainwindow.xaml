<Window x:Class="todolist_mvvm.Mainwindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:todolist_mvvm"
        mc:Ignorable="d"
        Title="To Do List" Height="600" Width="600" MinHeight="400" MinWidth="400">
    <Grid >
        <Button Name="back" Content="&lt;-" Click="Back_Click" Grid.Row="0" Grid.Column="0" Margin="10" Background="Transparent" FontSize="15" BorderThickness="0" VerticalAlignment="Top" HorizontalAlignment="Left"/>

        <Frame Name="MainFrame" NavigationUIVisibility="Hidden" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,40,0,0" />
    </Grid>
</Window>
